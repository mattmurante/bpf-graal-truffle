package BPF.Graal;

import java.io.File;

//Class to execute virtual machine on program file

public class VM {
	
    public static void main(String args[]) {
    	if (args.length != 1) {
    		return;
    	}
    	File infile = new File(args[0]);
    	FileInputStream fis = new FileInputStream(infile);
    }
}
